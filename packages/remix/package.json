{
  "name": "@tsi18n/remix",
  "version": "0.1.0",
  "description": "Simple Internationalization library",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "author": "Michel Smola <a@michelsmola.de>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "watch:esm": "tsc --watch",
    "watch:cjs": "tsc -p tsconfig.cjs.json --watch",
    "watch": "concurrently 'yarn watch:esm' 'yarn watch:cjs'",
    "lint": "prettier --write src && eslint --fix src/**/*.ts"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "remix": "^1.1.3",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "remix": "^1.1.1"
  },
  "dependencies": {
    "@tsi18n/react": "0.1.0",
    "jsesc": "^3.0.2",
    "tslib": "^2.3.1"
  },
  "sideEffects": false
}
